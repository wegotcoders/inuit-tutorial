<section class="layout__item layout__item--center lap-and-up-layout">
  <section role="main" class="lap-and-up-layout__item lap-and-up-two-thirds">
    <div class="palm-carousel__border palm-layout palm-mv-">
      <%= render 'demos/carousel' %>
    </div>

    <article role="article" class="main-article__border layout lap-and-up-mt mb-">
      <%= render 'demos/article' %>
    </article>

    <div class="palm-layout-eg-1__border palm-layout mb-">
      <%= render 'demos/layout_example_one' %>
    </div>

    <div class="palm-layout-eg-2__border palm-layout mb-">
      <%= render 'demos/layout_example_two' %>
    </div>

    <div class="palm-layout-eg-3__border palm-layout mb-">
      <%= render 'demos/layout_example_three' %>
    </div>
  </section><!--

  --><aside role="complementary" class="lap-and-up-one-third lap-and-up-layout__item pr0">
    <%= render 'demos/form' %>
    <%= render 'demos/flyout' %>
    <%= render 'demos/buttons' %>
  </aside>
</section>
